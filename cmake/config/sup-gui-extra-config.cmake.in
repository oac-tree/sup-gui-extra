#  SUP_GUI_EXTRA_INCLUDE_DIRS - include directories for qt-mvvm
#  SUP_GUI_EXTRA_LIBRARIES    - libraries to link against

get_filename_component(SUP_GUI_EXTRA_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

include(CMakeFindDependencyMacro)

find_dependency(QT NAMES Qt6 Qt5 REQUIRED COMPONENTS Quick Widgets Core Gui PrintSupport Test)

find_dependency(Threads)

if(TARGET sup-gui-extra::ksyntaxhighlighting OR TARGET sup-gui-extra::coa-icons)
else()
  message(STATUS "sup-gui-extra-config.cmake including ${SUP_GUI_EXTRA_CMAKE_DIR}/sup-gui-extra-targets.cmake")
  include("${SUP_GUI_EXTRA_CMAKE_DIR}/sup-gui-extra-targets.cmake")
endif()

set(SUP_GUI_EXTRA_LIBRARIES sup-gui-extra::ksyntaxhighlighting sup-gui-extra::coa-icons)

message(STATUS "sup-gui-extra-config.cmake in ${CMAKE_CURRENT_LIST_FILE}")
message(STATUS "SUP_GUI_EXTRA_LIBRARIES ${SUP_GUI_EXTRA_LIBRARIES}")
